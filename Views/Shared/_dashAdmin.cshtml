<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>ClickToBuy</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="Free HTML Templates" name="keywords">
    <meta content="Free HTML Templates" name="description">

    <!-- Favicon -->
    <link href="~/Theme/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">

    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/Theme/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/Theme/css/style.css" rel="stylesheet">
</head>
<body>
    <!-- Topbar Start -->
    <div class="container-fluid">
        <div class="row bg-secondary py-2 px-xl-5">
            <div class="col-lg-6 d-none d-lg-block">
                <div class="d-inline-flex align-items-center">
                    <a class="text-dark" href="">Contact</a>
                    <span class="text-muted px-2">|</span>
                    <a class="text-dark" href="">Aide</a>
                    <span class="text-muted px-2">|</span>
                    <a class="text-dark" href="">Support</a>
                </div>
            </div>
            <div class="col-lg-6 text-center text-lg-right">
                <div class="d-inline-flex align-items-center">
                    <div id="google_translate_element"></div>

                    <script type="text/javascript">
                        function googleTranslateElementInit() {
                            new google.translate.TranslateElement({ pageLanguage: 'fr', layout: google.translate.TranslateElement.InlineLayout.SIMPLE }, 'google_translate_element');
                        }
                    </script>

                    <script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
                    <a class="text-dark px-2" href="">
                        <i class="fab fa-facebook-f"></i>
                    </a>
                    <a class="text-dark px-2" href="">
                        <i class="fab fa-twitter"></i>
                    </a>
                    <a class="text-dark px-2" href="">
                        <i class="fab fa-linkedin-in"></i>
                    </a>
                    <a class="text-dark px-2" href="">
                        <i class="fab fa-instagram"></i>
                    </a>
                    <a class="text-dark pl-2" href="">
                        <i class="fab fa-youtube"></i>
                    </a>

                </div>
            </div>
            <div style="margin-left:900px; margin-top:-25px">
               

            </div>
        </div>
        <div class="row align-items-center py-3 px-xl-5">
            <div class="col-lg-3 d-none d-lg-block">
                <a href="" class="text-decoration-none">
                    <h1 class="mb-4 display-5 font-weight-semi-bold">Click<span class="text-primary font-weight-bold  ">To</span>Buy</h1>
                </a>
            </div>
            <div class="col-lg-6 col-6 text-left">
                <form  asp-controller="Admin" asp-action="recherche">
                    <div class="input-group">
                        <input type="text" class="form-control"  id="searchString" name="searchString" placeholder="Rechercher un utilisateur">
                        <div class="input-group-append">
                            <span class="input-group-text bg-transparent text-primary">
                                <a href="~/Admin/recherche"><i class="fa fa-search"></i></a>
</span>
                        </div>
                    </div>
                </form>
            </div>

        </div>
    </div>
    <!-- Topbar End -->
    <!-- Navbar Start -->
    <div class="container-fluid mb-5">
        <div class="row border-top px-xl-5">
            <div class="col-lg-3 d-none d-lg-block">

                <a class="btn shadow-none d-flex align-items-center justify-content-between bg-primary text-white w-100" data-toggle="collapse" href="#navbar-vertical" style="height: 65px;  padding: 0 30px;">
                    <h6 class="m-0">Liste des Utilisateurs</h6>

                </a>
                <nav class="collapse show navbar navbar-vertical navbar-light align-items-start p-0 border border-top-0 border-bottom-0" id="navbar-vertical">
                    <div class="navbar-nav w-100 overflow-hidden" style="height: 410px">
                        <div class="nav-item dropdown">
                            <a href="~/Admin/clients" class="nav-link">Clients </a>

                        </div>
                        <div class="nav-item dropdown">
                            <a href="~/Admin/propriétaires" class="nav-link">Propriétaires </a>

                        </div>




                    </div>
                </nav>

                <a class="btn shadow-none d-flex align-items-center justify-content-between bg-primary text-white w-100" data-toggle="collapse" href="#navbar-vertical" style="height: 65px; margin-top: -300px; padding: 0 30px;">
                    <h6 class="m-0">Historique Utilisateurs</h6>

                </a>
                <nav class="collapse show navbar navbar-vertical navbar-light align-items-start p-0 border border-top-0 border-bottom-0" id="navbar-vertical">
                    <div class="navbar-nav w-100 overflow-hidden" style="height: 410px">
                        <div>
                            <a href="~/Admin/historiquec" class="nav-link" >Clients <i class=" float-right mt-1"></i></a>

                        </div>
                        <div>
                            <a href="~/Admin/historiqueP" class="nav-link" >Propriétaires <i class=" float-right mt-1"></i></a>

                        </div>


                    </div>
                </nav>
                <a class="btn shadow-none d-flex align-items-center justify-content-between bg-primary text-white w-100" data-toggle="collapse" href="#navbar-vertical" style="height: 65px; margin-top: -300px; padding: 0 30px;">
                    <h6 class="m-0">Remarques/Problèmes</h6>

                </a>
                <nav class="collapse show navbar navbar-vertical navbar-light align-items-start p-0 border border-top-0 border-bottom-0" id="navbar-vertical">
                    <div class="navbar-nav w-100 overflow-hidden" style="height: 410px">
                        <div>
                            <a href="~/Admin/msg" class="nav-link" >Messages <i class=" float-right mt-1"></i></a>

                        </div>
                        


                    </div>
                </nav>
            </div>
            <div class="col-lg-9">
                <nav class="navbar navbar-expand-lg bg-light navbar-light py-3 py-lg-0 px-0">
                    <a href="" class="text-decoration-none d-block d-lg-none">
                        <h1 class="m-0 display-5 font-weight-semi-bold"><span class="text-primary font-weight-bold border px-3 mr-1">E</span>Shopper</h1>
                    </a>
                    <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-between" id="navbarCollapse">
                        <div class="navbar-nav mr-auto py-0">
                            <a href="~/Admin/statistiques" class="nav-item nav-link ">Statistiques</a>

                            <a href="~/Admin/addclient" class="nav-item nav-link">Ajouter un utilisateur</a>

                            <a href="~/Admin/favorisAdClt" class="nav-item nav-link">Mes favoris</a>
                            <a href="~/Admin/listenoire" class="nav-item nav-link">Liste noire</a>
                        </div>
                        <div class="navbar-nav ml-auto py-0">
                            <a href="~/Prop/profileProp/" class="nav-item nav-link">
                                @if (Session["adminId"] != null)
                                {@Session["adminprenom"].ToString() @Session["adminnom"].ToString()}
                            </a>
                            <div style="margin-top:-20px; margin-left:8px">
                                <a href="" class="nav-item nav-link"> @Html.ActionLink("Déconnexion", "Deconnecter")</a>
                            </div>
                        </div>
                    </div>
                </nav>
                <div class="container body-content">
                    @RenderBody()

                </div>
            </div>
        </div>
    </div>
    <!-- Navbar End -->
    <!-- Featured Start -->
    </div>
    <!-- Featured End -->
    <!-- Categories Start -->
    <!-- Categories End -->
    <!-- Offer Start -->
    <!-- Offer End -->
    <!-- Products Start -->
    <!-- Footer Start -->
    <div class="container-fluid bg-secondary text-dark mt-5 pt-5">
        <div class="row px-xl-5 pt-5">
            <div class="col-lg-4 col-md-12 mb-5 pr-3 pr-xl-5">
                <a href="" class="text-decoration-none">
                    <h1 class="mb-4 display-5 font-weight-semi-bold">Click<span class="text-primary font-weight-bold  ">To</span>Buy</h1>
                </a>
                <p>ClickToBuy est une plateforme commerciale en ligne qui mit en relation des millions d'utilisateurs avec des vendeurs du monde entier.</p>

            </div>
            <div class="col-lg-8 col-md-12">
                <div class="row">
                    <div class="col-md-4 mb-5">
                        <h5 class="font-weight-bold text-dark mb-4">LIENS RAPIDES</h5>
                        <div class="d-flex flex-column justify-content-start">
                            <a class="text-dark mb-2" href="index.html"><i class="fa fa-angle-right mr-2"></i>Home</a>
                            <a class="text-dark mb-2" href="shop.html"><i class="fa fa-angle-right mr-2"></i>Ajouter un utilisateur</a>



                            <a class="text-dark" href="contact.html"><i class="fa fa-angle-right mr-2"></i>Contact Us</a>
                        </div>
                    </div>
                    <div class="col-md-4 mb-5">
                        <h5 class="font-weight-bold text-dark mb-4">CONTACT</h5>
                        <div class="d-flex flex-column justify-content-start">
                            <p class="mb-2"><i class="fa fa-map-marker-alt text-primary mr-3"></i>123 Street, Paris, France</p>
                            <p class="mb-2"><i class="fa fa-envelope text-primary mr-3"></i>contact@clicktobuy.fr</p>
                            <p class="mb-0"><i class="fa fa-phone-alt text-primary mr-3"></i>+033 345 67890</p>
                        </div>
                    </div>
                    <div class="col-md-4 mb-5">
                        <h5 class="font-weight-bold text-dark mb-4">TROUVEZ-NOUS SUR</h5>
                        <div class="d-inline-flex align-items-center">
                            <a class="text-dark px-2" href="" style="font-size: 20px">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                            <a class="text-dark px-2" href="" style="font-size: 20px">
                                <i class="fab fa-twitter"></i>
                            </a>
                            <a class="text-dark px-2" href="" style="font-size: 20px">
                                <i class="fab fa-linkedin-in"></i>
                            </a>
                            <a class="text-dark px-2" href="" style="font-size: 20px">
                                <i class="fab fa-instagram"></i>
                            </a>
                            <a class="text-dark pl-2" href="" style="font-size: 20px">
                                <i class="fab fa-youtube"></i>
                            </a>
                            <a class="text-dark pl-2" href="" style="font-size: 20px">
                                <i class="fab fa-snapchat"></i>
                            </a>

                        </div>
                        <br />
                        <br />
                        <div class="d-inline-flex align-items-center" style="margin-left:30px">
                            <P class="font-weight-bold">APPLICATIONS</P>
                            <a class="text-dark pl-2" href="">
                                <i class="fab fa-apple" aria-hidden="true" style="margin-left:-100px; margin-top:60px; font-size:35px "></i>
                            </a>
                            <a class="text-dark pl-2" href="">
                                <i class="fab fa-android" aria-hidden="true" style="margin-left:-70px; margin-top:60px; font-size:35px "></i>
                            </a>
                        </div>

                    </div>
                </div>
            </div>
            <div class="row border-top border-light mx-xl-5 py-4">
                <div class="col-md-6 px-xl-0">
                    <p class="mb-md-0 text-center text-md-left text-dark">
                        &copy; <a class="text-dark font-weight-semi-bold" href="#">ClickToBuy</a>. Tous droits réservés.

                        <br />
                        <a href="https://themewagon.com" target="_blank">Politique de confidentialité         |       </a>
                        <a href="https://themewagon.com" target="_blank">Termes et conditions</a>
                    </p>
                </div>
                <div class="col-md-6 px-xl-0 text-center text-md-right" style="margin-left:600px;margin-top:-40px">
                    <img class="img-fluid" src="~/Theme/img/payments.png" alt="">
                </div>
            </div>
        </div>
        <!-- Footer End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-primary back-to-top"><i class="fa fa-angle-double-up"></i></a>


        <!-- JavaScript Libraries -->
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
        <script src="~/Theme/lib/easing/easing.min.js"></script>
        <script src="~/Theme/lib/owlcarousel/owl.carousel.min.js"></script>

        <!-- Contact Javascript File -->
        <script src="~/Theme/mail/jqBootstrapValidation.min.js"></script>
        <script src="~/Theme/mail/contact.js"></script>

        <!-- Template Javascript -->
        <script src="~/Theme/js/main.js"></script>
</body>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)

</html>


