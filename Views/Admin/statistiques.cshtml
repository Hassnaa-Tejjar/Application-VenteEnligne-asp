
@model aspprojet.Models.Tables

@{
    ViewBag.Title = "statistiques";
    Layout = "~/Views/Shared/_dashAdmin.cshtml";
}

<link href="~/statistic/css/sb-admin-2.css" rel="stylesheet" />

<!-- Content Row -->
<div class="row">

    <!-- Earnings (Monthly) Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
        <div class="card border-left-primary shadow h-100 py-2">
            <div class="card-body">
                <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                        <div class="text-xs font-weight-bold text-primary text-uppercase mb-1">
                            Utilisateur
                        </div>
                        <div class="text-xs font-weight-bold text-primary text-uppercase mb-1">
                            (Client)
                        </div>
                        <div class="h5 mb-0 font-weight-bold text-gray-800">@ViewBag.clt</div>
                    </div>
                    <div class="col-auto">
                        <i class="fas fa-user fa-2x text-gray-300"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Earnings (Monthly) Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
        <div class="card border-left-success shadow h-100 py-2">
            <div class="card-body">
                <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                        <div class="text-xs font-weight-bold text-success text-uppercase mb-1">
                            Utilisateur (Propriétaire)
                        </div>
                        <div class="h5 mb-0 font-weight-bold text-gray-800" style="margin-top:9px">@ViewBag.prop</div>
                    </div>
                    <div class="col-auto">
                        <i class="fas fa-user fa-2x text-gray-300"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Earnings (Monthly) Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
        <div class="card border-left-info shadow h-100 py-2">
            <div class="card-body">
                <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                        <div class="text-xs font-weight-bold text-info text-uppercase mb-1">
                            Produits/

                        </div>
                        <div class="text-xs font-weight-bold text-info text-uppercase mb-1">

                            Services
                        </div>
                        <div class="h5 mb-0 font-weight-bold text-gray-800">@ViewBag.prod</div>
                    </div>
                    <div class="col-auto">
                        <i class="fas fa-calendar fa-2x text-gray-300"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Pending Requests Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
        <div class="card border-left-warning shadow h-100 py-2">
            <div class="card-body">
                <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                        <div class="text-xs font-weight-bold text-warning text-uppercase mb-1">
                            Remarque/
                            Problème
                        </div>
                        <div class="h5 mb-0 font-weight-bold text-gray-800" style="margin-top:9px">@ViewBag.msg</div>
                    </div>
                    <div class="col-auto">
                        <i class="fas fa-comment fa-2x text-gray-300"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="">
    <div class="" style="margin-left:-30px;margin-top:30px">
        <div class="roww">
            <div class="col-lg-66 grid-margin stretch-cardd shadow">
                <div class="cardd">
                    <div class="cardd-body">
                        <h4 class="cardd-title">Clients</h4>
                        <p class="card-description">
                            <code>Récents</code>
                        </p>
                        <div class="table-responsive">
                            <table class="table">

                                <thead>
                                    <tr>
                                        <th style="">Nom Complet</th>

                                        <th>Email</th>

                                    </tr>
                                </thead>
                                @foreach (var item in Model.clients)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.nom) @Html.DisplayFor(modelItem => item.prenom)
                                        </td>


                                        <td>
                                            @Html.DisplayFor(modelItem => item.email)
                                        </td>



                                    </tr>
                                }

                            </table>
                        </div>
                    </div>

                </div>
                <div style="padding-left:20px;width:500px">
                    <div class="col-lg-67 grid-margin stretch-cardd shadow">
                        <div class="cardd ">
                            <div class="cardd-body">
                                <h4 class="cardd-title">Propriétaires</h4>
                                <p class="card-description">
                                    <code>Récents</code>
                                </p>
                                <div class="table-responsive">
                                    <table class="table" style="width: 430px">

                                        <thead>
                                            <tr>
                                                <th style="">Nom Complet</th>

                                                <th>Email</th>

                                            </tr>
                                        </thead>
                                        @foreach (var item in Model.proprietaires)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.nom) @Html.DisplayFor(modelItem => item.prenom)
                                                </td>


                                                <td>
                                                    @Html.DisplayFor(modelItem => item.email)
                                                </td>



                                            </tr>
                                        }

                                    </table>
                                </div>
                            </div>

                        </div>

                    </div>

                </div>

            </div>
        </div>

    </div>
</div>